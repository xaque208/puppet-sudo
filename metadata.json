{
  "name": "zleslie-sudo",
  "version": "0.3.0",
  "author": "zleslie",
  "summary": "A simple module for managing sudo",
  "license": "Apache-2.0",
  "source": "https://github.com/xaque208/puppet-sudo",
  "project_page": "https://github.com/xaque208/puppet-sudo",
  "issues_url": "https://github.com/xaque208/puppet-sudo/issues",
  "dependencies": [
    {"name":"puppetlabs-stdlib","version_requirement":">= 1.0.0"},
    {"name": "puppetlabs/concat","version_requirement": ">= 1.1.0"}
  ],
  "data_provider": "hiera",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "16.04",
        "14.04"
      ]
    },
    {
      "operatingsystem": "Solaris",
      "operatingsystemrelease": [
        "11",
        "10"
      ]
    },
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "10"
      ]
    },
    {
      "operatingsystem": "OpenBSD",
      "operatingsystemrelease": [
        "6.0",
        "5.9"
      ]
    }
  ]
}

